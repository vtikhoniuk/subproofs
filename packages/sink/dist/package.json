{
  "name": "@substreams/sink",
  "version": "0.5.0",
  "description": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git"
  },
  "sideEffects": false,
  "dependencies": {
    "@substreams/core": "workspace:*"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.4.2",
    "@connectrpc/connect": "^1.1.3",
    "effect": "2.0.0-next.54"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./metrics": {
      "types": "./dist/dts/metrics.d.ts",
      "import": "./dist/esm/metrics.js",
      "default": "./dist/cjs/metrics.js"
    },
    "./sink": {
      "types": "./dist/dts/sink.d.ts",
      "import": "./dist/esm/sink.js",
      "default": "./dist/cjs/sink.js"
    },
    "./stream": {
      "types": "./dist/dts/stream.d.ts",
      "import": "./dist/esm/stream.js",
      "default": "./dist/cjs/stream.js"
    }
  },
  "typesVersions": {
    "*": {
      "metrics": [
        "./dist/dts/metrics.d.ts"
      ],
      "sink": [
        "./dist/dts/sink.d.ts"
      ],
      "stream": [
        "./dist/dts/stream.d.ts"
      ]
    }
  }
}